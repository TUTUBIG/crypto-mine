# Cloudflare Configuration
cf_account=your_cloudflare_account_id
cf_namespace=your_cloudflare_namespace_id
cf_api_key=your_cloudflare_api_key
worker_host=https://your-worker.workers.dev
worker_token=your_worker_token

# Blockchain RPC Endpoints
rpc_endpoint=https://your-rpc-endpoint.com
ws_endpoint=wss://your-websocket-endpoint.com

# Token Price Configuration
eth_price=4180.6704

# Business Logic Configuration
min_volume_usd=1000.0
price_precision=10000.0
candle_interval=5m

# Performance Configuration
buffer_size=10000
load_timeout=30s
http_timeout=10s

# Debug mode
debug=true

